<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Date</th>
      <th>Attendees</th>
      <th>Notes</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% meetings.each do |meeting| %>
      <tr>
        <td><%= link_to meeting.title_display, club_meeting_path(club, meeting) %></td>
        <td><%= meeting.description %></td>
        <td><%= meeting.date_and_time %></td>
        <td><%= meeting.attendances_count %></th>
        <td><%= meeting.notes %></td>
        <td><%= link_to "Edit", edit_club_meeting_path(club, meeting) %></td>
        <td><%= link_to "Destroy", club_meeting_path(club, meeting), method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
