<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Arrival Time</th>
      <th>Departure Time</th>
    </tr>
  </thead>

  <tbody>
    <% attendances.each do |attendance| %>
      <tr>
        <td><%= link_to attendance.attendee.name, club_member_path(club, attendance.attendee) %></td>
        <td><%= attendance.arrival_time %></td>
        <td><%= attendance.departure_time %></td>
        <td><%= attendance.notes %></td>
        <td><%= link_to "Destroy", club_attendance_path(club, attendance), method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
